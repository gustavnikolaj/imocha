{
  "name": "imocha",
  "version": "0.0.2",
  "description": "Standalone watch mode for mocha.",
  "scripts": {
    "coverage": "nyc mocha",
    "lint": "eslint .",
    "test": "mocha",
    "test-watch": "./bin/mocha-watch"
  },
  "bin": {
    "mocha-watch": "bin/mocha-watch"
  },
  "keywords": [
    "mocha-watch",
    "mocha",
    "watch"
  ],
  "author": "Gustav Nikolaj <gustavnikolaj@gmail.com>",
  "repository": "github:gustavnikolaj/mocha-watch",
  "license": "MIT",
  "engines": {
    "node": ">=8.3.0"
  },
  "peerDependencies": {
    "mocha": ">2.2.4"
  },
  "devDependencies": {
    "coveralls": "^3.0.2",
    "eslint": "^5.4.0",
    "eslint-config-pretty-standard": "^2.0.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-prettier": "^2.6.2",
    "mocha": "^5.2.0",
    "nyc": "^12.0.2",
    "prettier": "^1.14.2",
    "unexpected": "^10.38.0"
  },
  "dependencies": {
    "@babel/parser": "^7.0.0-rc.3",
    "@babel/traverse": "^7.0.0-rc.3",
    "@gustavnikolaj/async-main-wrap": "^3.0.0",
    "chokidar": "^2.0.4",
    "commander": "2.15.1",
    "minimatch": "3.0.4"
